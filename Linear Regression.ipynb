   "source": [
    "# Determination coefficient"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "def score(X, y, theta):\n",
    "    a = np.sum((y - model(X,theta))**2)\n",
    "    b = np.sum((y - np.mean(y))**2 )    \n",
    "    return 1 - (a/b)"
   ]
  },
